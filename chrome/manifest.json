{
  "name": "Sync",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension sucks",
  "homepage_url": "http://google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery-2.1.3.min.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [
    "bookmarks",
    "history",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}